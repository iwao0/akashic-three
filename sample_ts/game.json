			"type": "script",
			"path": "script/_bootstrap.js",
			"global": true
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"parameterObject": {
			"type": "script",
			"path": "script/parameterObject.js",
			"global": true
		},
		"Scene3D": {
			"type": "script",
			"path": "script/Scene3D.js",
			"global": true
		},
